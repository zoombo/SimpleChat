<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Labelframe" id="mainFrame">
    <property name="borderwidth">5</property>
    <property name="text" translatable="yes">Hello world! :)</property>
    <layout>
      <property name="ipadx">0</property>
      <property name="ipady">0</property>
      <property name="propagate">True</property>
      <rows>
        <row id="0">
          <property name="minsize">0</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="tk.Text" id="messageViewer">
        <property name="height">15</property>
        <property name="padx">0</property>
        <property name="pady">0</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="messageEnterer">
        <property name="height">3</property>
        <property name="pady">0</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="sendButton">
        <property name="text" translatable="yes">Отправить</property>
        <property name="width">11</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="rowspan">1</property>
          <property name="sticky">nsew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="configButton">
        <property name="text" translatable="yes">Reconfigure</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="ipadx">0</property>
          <property name="ipady">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="rowspan">1</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="membersList">
        <property name="height">15</property>
        <property name="width">15</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">1</property>
          <property name="ipadx">0</property>
          <property name="ipady">0</property>
          <property name="padx">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="rowspan">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="statusLabel">
        <property name="font">{Calibri} 11 {bold}</property>
        <property name="text" translatable="yes">STATUS</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Scrollbar" id="messageViewer_scroll">
        <property name="orient">vertical</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nsw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Scrollbar" id="membersList_scroll">
        <property name="orient">vertical</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nsw</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
